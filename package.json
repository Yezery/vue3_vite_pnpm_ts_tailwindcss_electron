{
  "name": "electron-vue-vite",
  "version": "28.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "Ziv YE <1172029997@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build mac": "vue-tsc && vite build && electron-builder",
    "build win": "vue-tsc && vite build && electron-builder --win --arm64",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^22.10.1",
